<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="../../../../favicon.ico">

    <title>Landed Estates Database Administration</title>

#parse("_inc_header.vm")

  </head>

  <body>

#parse("_nav_header.vm")


	<main role="main" class="container-fluid full-height">
	<div class="row full-height">
	
	#if ($showSideCol=="true")
	
	##
	## Two column layout: Main content column (9/12 width)
	##
	<div class="col-md-9 mh-100">
	#set($templateFileName=$pageId+".vm")
	#parse ($templateFileName)
	</div>
	
	##
	## Two column layout: Right-side column
	##
	<div class="col-md-3 mh-100">
	#set($sideFileName=$pageId+".side.vm")
	#parse($sideFileName)
	#if ($revisionHistory && $revisionHistory.size() > 0 )
	<h3>Revision History</h3>
	<ul>
	#foreach ($o in $revisionHistory)
	<li> $o.modified</li>
	#end
	</ul>
	#end
	
	</div>
	
	
	#else
	
	##
	## One column layout: main content column (full width)
	##
	<div class="col-md-12 full-height" xstyle="background-color:yellow">
	#set($templateFileName=$pageId+".vm")
	#parse ($templateFileName)
	</div>
	
	#end

	</div><!-- /.row -->
	</main><!-- /.container -->
	
	#parse ("_inc_footer.vm")
	
  </body>
</html>
