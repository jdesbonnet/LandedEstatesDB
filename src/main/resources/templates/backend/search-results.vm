#parse ("/backend/_search_result_item.vm")

<h2>Search results for '$q'
#if ($filter)
(filtered for $filter)
#end
</h2>

<p>Found  $results.size() results.</p>

<ul>
#foreach ($r in $results) 
<li> 
#search_result_item($r)
</li>
#end
</ul>

#if ($nResults == 0) 
no results for search term '$q'
#end
