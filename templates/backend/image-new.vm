$breadcrumbs.add("Houses","house-list.jsp").add($house.name,"house-edit.jsp?id=$house.id").add("add image")

<form action="image-new-submit.jsp" 
enctype="multipart/form-data" method="POST">

<input type="hidden" name="house_id" value="${house.id}" />

<div class="form-group">
<label for="file1">Image File</label>
<input type="file" name="file1" id="file1" class="form-control"/>
or URL of image from the web:
<input type="text" class="form-control"
id="field_url" name="url" />
</div>

<div class="form-group">
<label for="field_caption">Caption</label>
<textarea name="caption" id="field_caption" class="form-control"
rows="5" cols="80"/></textarea>
</div>

<div class="form-group">
<input type="submit" class="btn btn-primary" value="Upload File" />
</div>

</form>
