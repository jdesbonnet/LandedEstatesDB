
<form action="estate-new-submit.jsp" method="POST">


<div class="form-group">
<label for="name">New estate name</label>
<input type="text" class="form-control" size="40" id="name" name="name"
value="$!estate.getName()" />
</div>

<div class="form-group">
<label for="project_phase">Project Phase</label>
<select id="project_phase" class="form-control" name="project_phase">
<option value="1">Phase 1: Connaught</option>
<option value="2">Phase 2: Munster</option>
<option value="3" selected="yes">Phase 3: Leinster</option>
</select>
</div>


<input class="btn btn-primary" type="submit" value="Next &gt;" />

</form>


